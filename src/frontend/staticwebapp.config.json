{
  "routes": [
    {
      "route": "/api/user",
      "allowedRoles": [
        "authenticated",
        "user"
      ]
    },
    {
      "route": "/api/admin",
      "allowedRoles": [
        "authenticated",
        "admin"
      ]
    }
  ],
  "auth": {
    "rolesSource": "/api/GetRoles",
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "https://login.microsoftonline.com/5d65bff5-aca6-4063-9d75-44a22ca02ddf/v2.0",
          "clientIdSettingName": "AZURE_CLIENT_ID",
          "clientSecretSettingName": "AZURE_CLIENT_SECRET"
        }
      }
    }
  },
  "platform": {
    "apiRuntime": "python:3.10"
  }
}