{
  "routes": [
    {
      "route": "/api/user",
      "allowedRoles": [
        "user"
      ]
    },
    {
      "route": "/api/master",
      "allowedRoles": [
        "admin"
      ]
    }
  ],
  "auth": {
    "identityProviders": {
      "customOpenIdConnectProviders": {
        "aadb2c": {
          "registration": {
            "clientIdSettingName": "AADB2C_PROVIDER_CLIENT_ID",
            "clientCredential": {
              "clientSecretSettingName": "AADB2C_PROVIDER_CLIENT_SECRET"
            },
            "openIdConnectConfiguration": {
              "wellKnownOpenIdConfiguration": "https://{{AZURE_TENANT_ID}}.b2clogin.com/{{AZURE_TENANT_ID}}.onmicrosoft.com/B2C_1_signupsignin1/v2.0/.well-known/openid-configuration"
            }
          },
          "login": {
            "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
            "scopes": [],
            "loginParameterNames": []
          }
        }
      }
    }
  },
  "platform": {
    "apiRuntime": "python:3.10"
  },
  "navigationFallback": {
    "rewrite": "/index.html"
  }
}